{
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "lib": ["ESNext"],
    "outDir": "dist",
    "sourceMap": true,
    "noUncheckedIndexedAccess": true,
    "target": "ES2020",
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "rootDir": "src",
    "baseUrl": "src"
  }
}
